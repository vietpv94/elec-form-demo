{
  "name": "electronformdemo",
  "description": "electron Squirrel demo!",
  "author": "IBCT JSC",
  "private": true,
  "version": "0.1.0",
  "main": "main.js",
  "scripts": {
    "start": "SET NODE_ENV=development & electron .",
    "build": "./node_modules/.bin/electron-packager . $npm_package_productName --out=builds --platform=win32 --arch=x64 --overwrite",
    "build:osx": "./node_modules/.bin/electron-packager . $npm_package_productName --out=builds --platform=darwin --arch=x64 --overwrite"
  },
  "devDependencies": {
    "electron": "^2.0.0",
    "electron-installer-squirrel-windows": "^1.3.0",
    "electron-packager": "^12.0.2",
    "electron-prebuilt": "^1.4.13",
    "electron-winstaller": "^2.6.4"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  }
}
